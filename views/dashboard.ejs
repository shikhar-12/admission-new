
    <!-- nav start-->
<%- include('./partials/header.ejs')%>
 <!-- nav end-->
<div id="mycaraousel " class="carousel slide mt-3" data-bs-ride="carousel">
<div class="carousel-inner">
<div class="carousel-item active">
<img src="../images/banner-1.jpg" style="width:1500px;height:400px"/>
</div>
<div class="carousel-item">
<img src="../images/banner-2.jpg" style="width:1500px;height:400px"/>
</div>
<div class="carousel-item">
<img src="../images/banner-3.jpg" style="width:1500px;height:400px"/>
</div>
</div>
<a href="#mycaraousel" class="carousel-control-prev" data-bs-slide="prev">
<span class="carousel-control-prev-icon"></span>
</a>
<a href="#mycaraousel" class="carousel-control-next" data-bs-slide="next">
<span class="carousel-control-next-icon"></span>
</a>
</div>
<div class="container">
<div class="row text-center">
<div class="col">
    <p class="h1 mt-3" >Register Your Course<p class="fs-5 pt-3">Course Registration means the official recording of a student's name to study a course or courses.</p></p>
</div>
</div>
</div>
<div class="container mt-4">
<div class= "row text-center">
<div class = "col-md-4 ">
<div class="card c1">
<img class="card-img-top" src="../images/card1.jpg" style="height:185px"/>
<div class="card-body">
<h4 class="card-title">B.tech</h4>
<p class="card-text">B.Tech is a professional undergraduate engineering degree</p>
</div> 
<% if(!bt){ %>
    <div class="card-footer border-top-0" style="background:transparent;">
        <button class="btn btn-primary" type="submit" data-bs-toggle="modal" data-bs-target="#generalmodal">Register</button>
        </div>
    <%}else{%>
        <div class="card-footer border-top-0" style="background:transparent;">
            <a href="/courseview/<%= bt._id%>" class="btn btn-success">View</a>
            </div>
        <% } %>
    
</div>
</div>
<div class = "col-md-4">
<div class="card c2">
<img class="card-img-top" src="../images/card2.jpg" style="height:185px"/>
<div class="card-body">
<h4 class="card-title">BCA</h4>
<p class="card-text">BCA is a three-year Bachelor's Degree in Computer Application</p>
</div> 
<% if(!bc){ %>
    <div class="card-footer border-top-0" style="background:transparent;">
        <button class="btn btn-primary" type="submit" data-bs-toggle="modal" data-bs-target="#generalmodal">Register</button>
        </div>
    <%}else{%>
        <div class="card-footer border-top-0" style="background:transparent;">
            <a href="/courseview/<%= bc._id%>" class="btn btn-success">View</a>
            </div>
        <% } %>
</div>
</div>
<div class = "col-md-4">
<div class="card c3">
<img class="card-img-top" src="../images/card3.jpg" style="height:185px"/>
<div class="card-body">
<h4 class="card-title">MCA</h4>
<p class="card-text">MCA is a professional post-graduation degree in computer science</p>
</div> 
<% if(!mc){ %>
    <div class="card-footer border-top-0" style="background:transparent;">
        <button class="btn btn-primary" type="submit" data-bs-toggle="modal" data-bs-target="#generalmodal">Register</button>
        </div>
    <%}else{%>
        <div class="card-footer border-top-0" style="background:transparent;">
            <a href="/courseview/<%= mc._id%>" class="btn btn-success">View</a>
            </div>
        <% } %>
</div>
</div>
</div>
</div>
