<!-- modal start -->
<div class="modal" id="generalmodal">
    <div class="modal-dialog">
    <div class="modal-content">
    <div class="modal-header badge bg-primary text-wrap">
        <h5 class="modal-title" >Fill Your Details</h5>
    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="close" ></button>
    </div>
    <div class="modal-body" >
        <form action="/UserFormResult" method="post" >
            <div class="text-center">
                <label for="namebox" class="form-label text-center" >Full Name</label>
            <input name="username" type="textbox" id = "namebox" placeholder="Name" class ="form-control"/>
            </div>
            <div class=" mt-3 text-center">
                <label for="emailbox" class="form-label text-center" >Email Address</label>
            <input name="email" type="email" id = "emailbox" placeholder="Email" class ="form-control"/>
            </div>
            <div class=" mt-3 text-center">
                <label for="mobilebox" class="form-label text-center" >Mobile Number</label>
            <input name="mobile" type="textbox" id = "mobilebox" class ="form-control" placeholder="Mobile Number"/>
            </div>
            <div class=" mt-3 text-center">
                <label for="dobbox" class="form-label text-center" >Date Of Birth</label>
            <input name="dob" type="date" id = "dobbox" class ="form-control"/>
            </div>
            <h5 class="text-center mt-3" >Select Gender</h5>
            <div class="form-check">
                <label for="genderbox1" class="form-check-label" >Male</label>
            <input type="radio" value="Male"  id = "genderbox1" name="gender"  class ="form-check-input"/>
            <br>
            <label for="genderbox2" class="form-check-label" >Female</label>
            <input type="radio" value="Female" name="gender"   class="form-check-input" id = "genderbox2"/>
            </div>
            <!-- <div class=" form-check">
            
            </div> -->
            <div class="text-center mt-3 " >
    <label for="addressbox" >Enter Address</label>
    <textarea name="address" class="form-control" placeholder="Address" id="addressbox" ></textarea>
            </div>
            <div class="text-center mt-3" >
                <label for="select1" class="form-label text-center" >Select College</label>
    <select name="college" class="form-select" id="select1" >
    <option value="ITM UNIVERSITY" >ITM UNIVERSITY</option>
    <option value="Mits College" >Mits College</option>
    <option value="Jiwaji UNIVERSITY" >Jiwaji UNIVERSITY</option>
    </select>
            </div>
            <div class="text-center mt-3" >
                <label for="select2" class="form-label text-center" >Select Cousre</label>
    <select name="course" class="form-select" id="select2" >
    <option selected>B.Tech</option>
    <option value="BCA" >BCA</option>
    <option value="MCA" >MCA</option>
    </select>
            </div>
            <div class="text-center mt-3" >
                <label for="select3" class="form-label text-center" >Select Branch</label>
    <select name="branch" class="form-select" id="select3" >
    <option selected value="Computer Science">Computer Science</option>
    </select>
            </div>
            </div>
            <div class="modal-footer justify-content-center">
              <!-- <a href="./course/display">SUBMIT</a> -->
           <!-- <a href="/coursedisplay"> <button type="submit" class="btn btn-primary">SUBMIT</button> </a> -->
           <button type="submit" class="btn btn-primary">SUBMIT</button>
              </div>
            </form>
            <!-- <a href="./course/display" class="text-center"><button  class="btn btn-primary ">SUBMIT</button></a> -->
    </div>
    </div>
    </div>
    <!-- modal end -->